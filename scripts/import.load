LOAD CSV
     FROM '../output/categories.csv'
     INTO postgresql://{{DB_USER}}:{{DB_PASSWORD}}@{{DB_HOST}}:{{DB_PORT}}/{{DB_NAME}}
     TARGET TABLE categories
     WITH
          skip header = 1,
          fields terminated by ',',
          fields optionally enclosed by '"',
          null if '';

